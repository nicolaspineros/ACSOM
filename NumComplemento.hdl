	Not (in = ah[14], out = dig29);

	HalfAdder(a = dig0 , b = true, sum = sl[0], carry = carry0);
	HalfAdder(a = dig1 , b = carry0, sum = sl[1], carry = carry1);
	HalfAdder(a = dig2 , b = carry1, sum = sl[2], carry = carry2);
	HalfAdder(a = dig3 , b = carry2, sum = sl[3], carry = carry3);
	HalfAdder(a = dig4 , b = carry3, sum = sl[4], carry = carry4);
	HalfAdder(a = dig5 , b = carry4, sum = sl[5], carry = carry5);
	HalfAdder(a = dig6 , b = carry5, sum = sl[6], carry = carry6);
	HalfAdder(a = dig7 , b = carry6, sum = sl[7], carry = carry7);
	HalfAdder(a = dig8 , b = carry7, sum = sl[8], carry = carry8);
	HalfAdder(a = dig9 , b = carry8, sum = sl[9], carry = carry9);
	HalfAdder(a = dig10 , b = carry9, sum = sl[10], carry = carry10);
	HalfAdder(a = dig11 , b = carry10, sum = sl[11], carry = carry11);
	HalfAdder(a = dig12 , b = carry11, sum = sl[12], carry = carry12);
	HalfAdder(a = dig13 , b = carry12, sum = sl[13], carry = carry13);
	HalfAdder(a = dig14 , b = carry13, sum = sl[14], carry = carry14);

	HalfAdder(a = dig15 , b = carry14, sum = sh[0], carry = carry15);
	HalfAdder(a = dig16 , b = carry15, sum = sh[1], carry = carry16);
	HalfAdder(a = dig17 , b = carry16, sum = sh[2], carry = carry17);
	HalfAdder(a = dig18 , b = carry17, sum = sh[3], carry = carry18);
	HalfAdder(a = dig19 , b = carry18, sum = sh[4], carry = carry19);
