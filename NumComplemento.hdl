CHIP NumComplemento{

	IN al[15], ah[15];
	OUT sl[15], sh[15];

	PARTS:
	
	Not(in = al[0], out = dig0);
	Not(in = al[1], out = dig1);
	Not(in = al[2], out = dig2);
