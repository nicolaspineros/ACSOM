	HalfAdder(a = dig24 , b = carry23, sum = sh[9], carry = carry24);
	HalfAdder(a = dig25 , b = carry24, sum = sh[10], carry = carry25);
	HalfAdder(a = dig26 , b = carry25, sum = sh[11], carry = carry26);
	HalfAdder(a = dig27 , b = carry26, sum = sh[12], carry = carry27);
